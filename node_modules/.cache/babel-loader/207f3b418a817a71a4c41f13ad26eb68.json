      artista: {},\n      sobreOvideo: null,\n      destinatario: false,\n      PedidoDTO: {\n        idArtista: null,\n        idUsuario: null,\n        destinatario: null,\n        conteudoVideo: null,\n        dtPedido: null,\n        status: null\n      }\n    };\n  },\n  methods: {\n    classicModalHide: function classicModalHide() {\n      this.openModal = false;\n    },\n    sucesso: function sucesso() {\n      this.showDialog = false, this.openModal = true;\n    },\n    finalizarCompra: function finalizarCompra() {\n      this.PedidoDTO.destinatario = this.destinatario;\n      this.PedidoDTO.idArtista = this.$route.params.id;\n      this.PedidoDTO.conteudoVideo = this.sobreOvideo;\n      this.$http.post('pedidos/salvar', this.PedidoDTO).then(function (res) {\n        return console.log(res);\n      });\n    }\n  },\n  mounted: function mounted() {\n    var _this = this;\n\n    this.$http.get('artistas/?id=' + this.$route.params.id).then(function (response) {\n      return _this.artista = response.data;\n    });\n  }\n};",{"version":3,"sources":["DetalhesItem.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoIA,OAAA,WAAA,MAAA,0BAAA;AACA,OAAA,UAAA,MAAA,qBAAA;AACA,SAAA,KAAA,QAAA,cAAA;AACA,SAAA,OAAA,QAAA,cAAA;AACA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,WAAA,EAAA,WADA;AAEA,IAAA,UAAA,EAAA,UAFA;AAGA,IAAA,KAAA,EAAA,KAHA;AAIA,IAAA,OAAA,EAAA;AAJA,GADA;AAOA,EAAA,IAAA,EAAA;AAAA,WAAA;AACA,MAAA,UAAA,EAAA,KADA;AAEA,MAAA,QAAA,EAAA,IAFA;AAGA,MAAA,SAAA,EAAA,KAHA;AAIA,MAAA,OAAA,EAAA,EAJA;AAKA,MAAA,WAAA,EAAA,IALA;AAMA,MAAA,YAAA,EAAA,KANA;AAOA,MAAA,SAAA,EAAA;AACA,QAAA,SAAA,EAAA,IADA;AAEA,QAAA,SAAA,EAAA,IAFA;AAGA,QAAA,YAAA,EAAA,IAHA;AAIA,QAAA,aAAA,EAAA,IAJA;AAKA,QAAA,QAAA,EAAA,IALA;AAMA,QAAA,MAAA,EAAA;AANA;AAPA,KAAA;AAAA,GAPA;AAwBA,EAAA,OAAA,EAAA;AACA,IAAA,gBADA,8BACA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,KAHA;AAIA,IAAA,OAJA,qBAIA;AACA,WAAA,UAAA,GAAA,KAAA,EACA,KAAA,SAAA,GAAA,IADA;AAEA,KAPA;AAQA,IAAA,eARA,6BAQA;AACA,WAAA,SAAA,CAAA,YAAA,GAAA,KAAA,YAAA;AACA,WAAA,SAAA,CAAA,SAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,WAAA,SAAA,CAAA,aAAA,GAAA,KAAA,WAAA;AAEA,WAAA,KAAA,CAAA,IAAA,CAAA,gBAAA,EAAA,KAAA,SAAA,EAAA,IAAA,CAAA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OAAA;AAEA;AAfA,GAxBA;AAyCA,EAAA,OAzCA,qBAyCA;AAAA;;AACA,SAAA,KAAA,CACA,GADA,CACA,kBAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,aAAA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA;AAAA,KAFA;AAGA;AA7CA,CAAA","sourcesContent":["<template> \n  <div class=\"fundo\"> \n    \n  <div class=\"md-layout md-gutter md-alignment-center\" style=\"margin-left:1px; margin-bottom:2px;\"> \n \n      <div class=\"md-layout-item md-medium-size-33 md-small-size-50 md-xsmall-size-100\" style=\"margin-left:200px\"> \n        <video-player videoWith=\"100%\" videoHeight=\"400px\" :src= \"artista.urlVideoPrincipal\" /> \n      </div> \n    <div class=\"md-layout-item md-medium-size-33 md-small-size-50 md-xsmall-size-100\" style=\"margin-top:0px !important;\"> \n      <h2> \n          <br /> \n          <b>{{artista.nomeArtista}}</b> \n        </h2> \n        <h6>{{tituloProfissao}}</h6> \n        <md-button class=\"md-raised md-primary\" @click=\"openModal = true\"> \n          <h6 class=\"tituloBotao\"> \n            <img src=\"@/assets/gif/Ovideo.gif\" style=\"width:80px;\" /> \n            por R$ \n            <strong>{{artista.valor}},00</strong> \n          </h6> \n        </md-button> <br> \n        <md-icon class=\"star\">star</md-icon> \n        <md-icon class=\"star\">star</md-icon> \n        <md-icon class=\"star\">star</md-icon> \n        <md-icon class=\"star\">star</md-icon> \n        <md-icon class=\"star\">star</md-icon> \n        <br /> \n        \n        <h6>Tempo previsto de resposta: {{tempoResposta}}</h6> <md-icon class=\"timer\">timer</md-icon> \n        <md-content> \n          <h6 class=\"descricao\"> \n            {{artista.descricaoPrincipal}} \n          </h6> \n        </md-content> \n  \n     \n    </div> \n    <div class=\"md-layout-item md-medium-size-33 md-small-size-10 md-xsmall-size-10\"></div> \n \n   <div class=\"md-layout-item md-size-100 md-alignment-center\"> \n      <br /> \n      <md-divider /> \n      <md-divider /> \n      <br /> \n      <h3 style=\"text-align:center; padding-bottom:8px;\"> \n        Veja outros vídeos \n        <br /> \n      </h3> \n    </div> \n \n  </div> \n     <div class=\"md-layout md-gutter md-alignment-center-center\" style=\"margin-left:20px;\" v-for=\"item in 2\" :key=item > \n    <div class=\"md-layout-item md-medium-size-33 md-small-size-50 md-xsmall-size-100 \" style=\"margin:8px\" v-for=\"video in artista.urlVideos\" :key=\"video\"> \n        <VideoPlayer videoWith=\"300px\" videoHeight=\"300px\" :src=\"video\" ></VideoPlayer> \n    </div> \n  </div> \n     \n  <!--- ####################### OPEN MODAL #################################--> \n \n      <modal v-if=\"openModal\" @close=\"classicModalHide\"> \n        <template slot=\"header\"> \n          <h3 class=\"modal-title\">Confirmação de Compra</h3> \n          <md-button \n            class=\"md-simple md-just-icon md-round modal-default-button\" \n            @click=\"classicModalHide\" \n          > \n            <md-icon>clear</md-icon> \n          </md-button> \n        </template> \n \n        <template slot=\"body\"> \n           \n          <!-- Primeiro bloco - informações sobre o vídeo --> \n          <div class=\"md-layout md-gutter\"> \n            <div class=\"md-layout-item md-medium-size-33 md-small-size-50 md-xsmall-size-100\" > \n              <h5>Esse vídeo é para quem ?</h5> \n              <md-radio v-model=\"destinatario\" value=\"mySelf\">Para mim</md-radio> \n              <md-radio v-model=\"destinatario\" value=\"someone\">Para Alguém</md-radio> \n              <md-radio v-model=\"destinatario\" value=\"marketing\">Para uma marca ou empresa</md-radio> \n            </div> \n            \n          </div> \n          <div class=\"md-layout md-gutter\"> \n            <div class=\"md-layout-item md-medium-size-33 md-small-size-50 md-xsmall-size-100\" > \n              <h5>O que você deseja ser dito no vídeo</h5> \n              <md-field> \n                <md-input v-model=\"sobreOvideo\" placeholder=\"Descreva o conteúdo do vídeo\"></md-input> \n              </md-field> \n            </div> \n        \n          </div> \n        <!-- Primeiro bloco - informações do Cartão --> \n          <div class=\"md-layout md-gutter\"> \n             \n              <div class=\"md-layout-item md-medium-size-33 md-small-size-50 md-xsmall-size-100\" > \n                <h5>Dados para Compra</h5> \n                <md-field> \n                  <md-input v-model=\"initial\" placeholder=\"Número do Cartao\"></md-input> \n                </md-field> \n                <md-field> \n                  <md-input v-model=\"initial\" placeholder=\"Código de Segurança\"></md-input> \n                </md-field> \n              </div> \n              <div class=\"md-layout-item md-medium-size-33 md-small-size-50 md-xsmall-size-100\" > \n                <md-datepicker v-model=\"selectedDate\"> \n                  <label>Selecione a data</label> \n                </md-datepicker> \n              </div> \n           \n          </div> \n         \n         \n        </template> \n \n        <template slot=\"footer\"> \n          <md-button class=\"md-simple\" @click=\"finalizarCompra()\">Finalizar Compra</md-button> \n          <md-button \n            class=\"md-danger md-simple\" \n            @click=\"classicModalHide\" \n            >Cancelar</md-button \n          > \n        </template> \n      </modal> \n \n \n \n \n  </div> \n \n</template> \n \n<script> \nimport VideoPlayer from \"@/components/VideoPlayer\"; \nimport MainNavbar from \"@/layout/MainNavbar\"; \nimport { Modal } from \"@/components\"; \nimport {TheMask} from 'vue-the-mask' \nexport default { \n  components: { \n    VideoPlayer, \n    MainNavbar, \n    Modal, \n    TheMask \n  }, \n  data: () => ({ \n    showDialog: false, \n    required : true, \n    openModal: false, \n    artista: {}, \n    sobreOvideo: null, \n    destinatario: false, \n    PedidoDTO:{ \n      idArtista:null, \n      idUsuario:null, \n      destinatario:null, \n      conteudoVideo:null, \n      dtPedido:null, \n      status:null \n    } \n  }), \n \n  methods: { \n    classicModalHide() { \n      this.openModal = false; \n    }, \n    sucesso(){ \n      this.showDialog = false, \n      this.openModal = true \n    }, \n    finalizarCompra(){ \n      this.PedidoDTO.destinatario = this.destinatario \n      this.PedidoDTO.idArtista = this.$route.params.id \n      this.PedidoDTO.conteudoVideo = this.sobreOvideo \n \n      this.$http.post('pedidos/salvar', this.PedidoDTO).then(res => console.log(res)); \n \n    } \n  }, \n  mounted () { \n    this.$http \n      .get('artistas/?id='+ this.$route.params.id) \n      .then(response => (this.artista = response.data)) \n  } \n \n}; \n</script> \n \n<style scoped> \n.descricao { \n  font-family: Arial, Helvetica, sans-serif; \n  text-transform: none; \n  font-size: 18px;} \n \nh6 { \n  color: black !important; \n} \n.star { \n  color: yellow !important; \n} \n \n.timer { \n  color: blue !important; \n} \n \n \n \n.md-dynamic-height .md-label { \n  color: black !important; \n  font-size: 18px !important; \n} \n.md-tabs .md-acento  .md-tabs-wrapper  .md-tabs-canvas  .md-paginação-wrapper .md-ink-bar { \n     color : #60c9f8 ; /* isso altera a cor da ondulação */  \n    background-color : #60c9f8 ;  /* isso muda a cor da barra de tinta */  \n} \n \n.fundo{ \n  background-color: white \n} \n</style> "],"sourceRoot":"src/views/myComponents"}]}